    <title>Search Page</title>
  </head>
  <body>
  
  <div class="container mt-4">
  
  <form action="search" method="post">
  
  	<div class="card bg-dark">
  	<div class="card-body text-center">
  	<h1 class="card-title text-center mt-5 p-2 text-white  fa fa-globe fa-5x fa-spin"></h1> <br>	
  	<input class="mt-2 " type="text" name="searchbox" placeholder="Search here...." >
  	<div class="text-center">
  	<button type="submit" class="btn btn-outline-light mt-3 ">Search</button>
  	</div>
  	</div>
  	
  	</div>
  
  </form>
  
  </div>
    

